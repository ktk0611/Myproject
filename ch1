package myproject;

public class NumberTypeDemo {

	public static void main(String[] args) {
		int my_var;
		long my_long;
		short my_shut;
		byte my_byte;
		float myDouble;
		char myChar;
		boolean myBoolean;
		
		my_var = 10;
		my_long = 10L;
		my_shut = 100;
		myChar = '0';
		
		myBoolean = true;
		
	
	}

}
